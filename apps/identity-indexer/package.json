{
  "name": "@ucp/identity-indexer",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run env:generate && tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "infra:check": "make -C ../.. infra-check",
    "env:generate": "tsx scripts/generate-env-schema.ts",
    "shovel:config": "tsx shovel/shovel-config.ts -- shovel/generated/ucp.local.json",
    "shovel:up": "./shovel/shovel.sh up",
    "shovel:down": "./shovel/shovel.sh down",
    "shovel:logs": "./shovel/shovel.sh logs",
    "generate:db-types": "tsx scripts/generate-db-types.ts",
    "register-agent": "tsx scripts/register_agent.ts",
    "verify-agent": "tsx scripts/verify_agent.ts",
    "postinstall": "pnpm run env:generate"
  },
  "dependencies": {
    "@indexsupply/shovel-config": "catalog:",
    "@ucp/config": "workspace:*",
    "@ucp-js/sdk": "workspace:*",
    "@ucp/erc8004-specs": "workspace:*",
    "@ucp/onchain": "workspace:*",
    "@hono/node-server": "catalog:",
    "@hono/zod-validator": "catalog:",
    "hono": "catalog:",
    "abitype": "catalog:",
    "dotenv": "catalog:",
    "kysely": "catalog:",
    "pg": "catalog:",
    "postgres-interval": "^4.0.2",
    "viem": "catalog:",
    "zod": "catalog:",
    "ox": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "kysely-codegen": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
