{
  "name": "@ucp/config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./env": "./src/env.ts",
    "./infra/shovel-config": "./src/infra/shovel-config.ts",
    ".": "./src/index.ts"
  },
  "types": "./src/index.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "check": "tsc -p tsconfig.json --noEmit",
    "generate-shovel-config": "TSX_DISABLE_CACHE=1 node --import tsx src/infra/shovel-config.ts",
    "shovel-abi-check": "TSX_DISABLE_CACHE=1 node --import tsx src/infra/shovel-abi-check.ts"
  },
  "dependencies": {
    "@indexsupply/shovel-config": "catalog:",
    "abitype": "catalog:",
    "dotenv": "catalog:",
    "ox": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
